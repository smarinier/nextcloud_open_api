{
	"name": "nextcloud_open_api",
	"version": "0.1.0",
	"description": "Place this app in **nextcloud/apps/**",
	"type": "module",
	"main": "src/main.ts",
	"private": true,
	"directories": {
		"test": "tests"
	},
	"scripts": {
		"build": "vite --mode production build",
		"dev": "vite --mode development build",
		"docs": "vendor/nextcloud/openapi-extractor/generate-spec",
		"watch": "vite --mode development build --watch",
		"lint": "eslint --ext .js,.vue src",
		"lint:fix": "eslint --ext .js,.vue src --fix",
		"stylelint": "stylelint src/**/*.vue",
		"stylelint:fix": "stylelint src/**/*.vue --fix"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/smarinier/nextcloud_open_api.git"
	},
	"keywords": [
		"swagger openapi rest api"
	],
	"author": "Sebastien Marinier <seb@smarinier.net>",
	"license": "AGPL-3.0",
	"require": {
		"php": "^8.0"
	},
	"bugs": {
		"url": "https://github.com/smarinier/nextcloud_open_api/issues"
	},
	"homepage": "https://github.com/smarinier/nextcloud_open_api",
	"browserslist": [
		"extends @nextcloud/browserslist-config"
	],
	"engines": {
		"node": ">= 20.0.0",
		"npm": "^10.0.0"
	},
	"dependencies": {
		"@mdi/svg": "^7.4.47",
		"@nextcloud/auth": "^2.4.0",
		"@nextcloud/axios": "^2.5.1",
		"@nextcloud/dialogs": "^5.3.7",
		"@nextcloud/files": "^3.9.0",
		"@nextcloud/initial-state": "^2.2.0",
		"@nextcloud/l10n": "^3.1.0",
		"@nextcloud/router": "^3.0.1",
		"@nextcloud/vue": "^8.19.0",
		"lodash": "^4.17.21",
		"pinia": "^2.2.4",
		"swagger-ui": "^5.17.14",
		"vue": "^2.7.16",
		"vue-material-design-icons": "^5.3.1",
		"vuex": "^3.6.2"
	},
	"devDependencies": {
		"@nextcloud/babel-config": "^1.0.0",
		"@nextcloud/browserslist-config": "^3.0.1",
		"@nextcloud/eslint-config": "^8.4.1",
		"@nextcloud/stylelint-config": "^3.0.1",
		"@nextcloud/vite-config": "^1.4.2",
		"@vue/cli-plugin-unit-jest": "5.0.6",
		"@vue/test-utils": "1.3.4",
		"@vue/vue2-jest": "^27.0.0",
		"typescript": "^5.6.3",
		"vite": "^5.4.8"
	}
}
