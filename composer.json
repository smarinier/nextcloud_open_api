{
	"name": "nextcloud/open_api",
	"config": {
        "optimize-autoloader": true,
        "classmap-authoritative": true
    },
    "autoload":{
        "psr-4": {
            "OCA\\OpenApiUI\\" : "lib/"
        }
    },
    "require-dev": {
		"nextcloud/openapi-extractor": "^1.0",
        "nextcloud/coding-standard": "^1.1"
	},
	"scripts": {
		"cs:check": "php-cs-fixer fix --dry-run --diff",
		"cs:fix": "php-cs-fixer fix"
	}
}
